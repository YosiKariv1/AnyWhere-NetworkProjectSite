@using AnyWhere____Network_Project_Website.Models

@{
    ViewBag.Title = "UserTable";
    Layout = "~/Views/Admin/DashBoard.cshtml";
}

<div class="tab-content">
    <div class="tab-pane container active p-0" id="home">
        <h5 class="text-uppercase p-2 text-center">List of Users</h5>
        <table class="table table-bordered table-striped">
            <thead class="thead-dark text-black">
                <tr>
                    <th>Id</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Password</th>
                </tr>
            </thead>
            <tbody>
                @foreach (Users su in Model.users)
                {
                    <tr>
                        <td>@su.Id</td>
                        <td>@su.FirstName</td>
                        <td>@su.LastName</td>
                        <td>@su.Email</td>
                        <td>@su.Password</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
